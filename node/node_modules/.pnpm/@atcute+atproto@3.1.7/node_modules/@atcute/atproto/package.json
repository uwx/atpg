{
  "type": "module",
  "name": "@atcute/atproto",
  "version": "3.1.7",
  "description": "AT Protocol (com.atproto.*) schema definitions",
  "keywords": [
    "atcute",
    "atproto"
  ],
  "license": "0BSD",
  "repository": {
    "url": "https://github.com/mary-ext/atcute",
    "directory": "packages/definitions/atproto"
  },
  "files": [
    "dist/",
    "lib/",
    "!lib/**/*.bench.ts",
    "!lib/**/*.test.ts"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./types/*": "./dist/lexicons/types/com/atproto/*.js"
  },
  "dependencies": {
    "@atcute/lexicons": "^1.2.2"
  },
  "devDependencies": {
    "@atcute/atproto": "file:",
    "@atcute/lex-cli": "^2.2.2"
  },
  "scripts": {
    "build": "tsc",
    "generate": "rm -r ./lib/lexicons/; lex-cli generate -c ./lex.config.js",
    "prepublish": "rm -rf dist; pnpm run build"
  }
}