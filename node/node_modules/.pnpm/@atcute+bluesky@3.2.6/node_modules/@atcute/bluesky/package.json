{
  "type": "module",
  "name": "@atcute/bluesky",
  "version": "3.2.6",
  "description": "Bluesky (app.bsky.* and chat.bsky.*) schema definitions",
  "keywords": [
    "atcute",
    "atproto",
    "bluesky"
  ],
  "license": "0BSD",
  "repository": {
    "url": "https://github.com/mary-ext/atcute",
    "directory": "packages/definitions/bluesky"
  },
  "files": [
    "dist/",
    "lib/",
    "!lib/**/*.bench.ts",
    "!lib/**/*.test.ts"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./types/app/*": "./dist/lexicons/types/app/bsky/*.js",
    "./types/chat/*": "./dist/lexicons/types/chat/bsky/*.js"
  },
  "dependencies": {
    "@atcute/atproto": "^3.1.7",
    "@atcute/lexicons": "^1.2.2"
  },
  "devDependencies": {
    "@atcute/bluesky": "file:",
    "@atproto/api": "^0.15.27",
    "vitest": "^3.2.4",
    "@atcute/lex-cli": "^2.2.2"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "test": "vitest",
    "generate": "rm -r ./lib/lexicons/; lex-cli generate -c ./lex.config.js",
    "prepublish": "rm -rf dist; pnpm run build"
  }
}